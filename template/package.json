{
  "name": "scifr",
  "version": "0.1.0",
  "description": "A scifr boilerplate",
  "author": "Budi Permana",
  "license": "MIT",
  "dependencies": {
    "@nivo/core": "^0.87.0",
    "@nivo/scatterplot": "^0.87.0",
    "jotai": "^2.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "dev": "react-app-rewired start",
    "build": "npm-run-all build-and-rename",
    "build-and-rename": "react-app-rewired build && cp build/index.html ./scifr.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "daisyui": "^4.12.2",
    "html-inline-css-webpack-plugin": "^1.11.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.0",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.2.1"
  }
}